{
  "mcpServers": {
    "git": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ],
      "env": {}
    },
    "electric-vehicle-data-server": {
      "type": "stdio",
      "command": ".venv/bin/mcp",
      "args": [
        "run",
        "mcp/example_server/electric_vehicle_data_server.py"
      ],
      "env": {}
    },
    "time": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ],
      "env": {}
    }
  }
}